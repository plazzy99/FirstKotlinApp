<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vatsal.kesarwani.a30daysofkotlin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".views.Activtiy.FourthActivity"
            android:parentActivityName=".views.Activtiy.ThirdActivity"
            android:label="Winner Tracker">
        </activity>
        <activity
            android:name=".views.Activtiy.ThirdActivity"
            android:label="Score Counter"
            android:parentActivityName=".views.Activtiy.SecondActiviity">
        </activity>
        <activity
            android:name=".views.Activtiy.SecondActiviity"
            android:label="Dice Generator"
            android:parentActivityName=".views.Activtiy.MainActivity" />
        <activity android:name=".views.Activtiy.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>